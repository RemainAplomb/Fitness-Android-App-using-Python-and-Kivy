#========== WORKOUT SCREEN ==========#
# THIS IS THE SCREEN THAT GUIDES THE 
# USER IN HIS WORKOUT ROUTINE. ANIMATED 
# GIF OF THE ACTION NEEDED TO BE DONE
# IS SHOWN IN THIS SCREEN.
<WorkoutScreen>:
	GridLayout:
		cols: 1
		FloatLayout:
			canvas.before:
				Rectangle:
					pos: self.pos
					size: self.size
					source: "resources/backgrounds/headline_background.jpg"
			size_hint: 1, 0.08
			Image:
				id: workout_lbl
				source: "resources/exercises/rest/exercise_lbl.jpg"
				size_hint: 0.5, 1
				pos_hint: { "x" : 0.25, "y" : 0 }
				allow_stretch: True
				keep_ratio: False

		FloatLayout:
			canvas.before:
				Rectangle:
					pos: self.pos
					size: self.size
					source: "resources/backgrounds/report_background.jpg"	
			size_hint: 1, 0.90
			Image:
				id: workout_img
				source: "resources/exercises/rest/1.jpg"
				anim_delay: 0.5
				size_hint: 1, 1
				pos_hint: { "x" : 0, "y" : 0 }
				allow_stretch: True
				keep_ratio: False
				keep_data: True
			ImageButton:
				id: cancel_BTN
				source: "resources/buttons/cancel.jpg"
				size_hint: 1, 0.1
				#background_color: 0, 0, 0, 0
				pos_hint: { "x" : 0 , "y" : 0 }
				allow_stretch: True
				keep_ratio: False
				on_release:
					app.cancel_workout()
					#app.root.current = "main_screen"
